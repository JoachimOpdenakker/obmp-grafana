      "targets": [
        {
          "alias": "",
          "format": "time_series",
          "hide": false,
          "rawSql": "SELECT\n    floor(extract(epoch from last(interval_time,interval_time))) as time,\n    CASE WHEN v4_prefixes > 700000 THEN 'full (> 700k)' \n            WHEN v4_prefixes > 300000 THEN 'partial (> 300K)'\n            WHEN v4_prefixes > 100000 THEN 'weak (> 100K)'\n            WHEN v4_prefixes > 9000 THEN 'poor (> 9k)'\n            ELSE 'customer_only (< 9K)'\n        END as rib_class,\n    count(distinct peer_hash_id) as count\nFROM stats_peer_rib s\n  JOIN bgp_peers p ON (p.hash_id = s.peer_hash_id)\nWHERE interval_time >= now() - interval '40 minute'\n  AND p.isipv4 = true\nGROUP BY rib_class\nORDER BY count desc\n\n",
          "refId": "A"
        }
      ],
      "title": "IPv4 Peer RIB Sizes",
      "type": "grafana-piechart-panel",
      "valueName": "current"
    },
